{
    "manifest_version": 3,
    "name": "chrome-extension-dataworks-enhance",
    "description": "chrome-extension-dataworks-enhance",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/idb-keyval.js",
                "scripts/content.js"
            ],
            "matches": [
                "https://*.data.aliyun.com/?defaultProjectId=*",
                "https://developer.chrome.com/docs/webstore/*"
            ]
        }
    ]
}